{
  "name": "shoekstra-owncloud",
  "version": "0.4.0",
  "author": "shoekstra",
  "summary": "Puppet module for ownCloud",
  "license": "Apache-2.0",
  "source": "https://github.com/shoekstra/puppet-owncloud.git",
  "project_page": "https://github.com/shoekstra/puppet-owncloud",
  "issues_url": "https://github.com/shoekstra/puppet-owncloud/issues",
  "description": "Puppet module to install and configure ownCloud",
  "tags": [
    "owncloud"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": [
        "19",
        "20"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/apache",
      "version_requirement": ">= 1.0.1 < 2.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 2.0.0 < 3.0.0"
    },
    {
      "name": "puppetlabs/mysql",
      "version_requirement": ">= 2.3.1 < 4.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 2.4.0 < 5.0.0"
    },
    {
      "name": "stahnma/epel",
      "version_requirement": ">= 1.0.0 <= 1.0.2"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }
  ]
}
